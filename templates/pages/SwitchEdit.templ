
package pages

import (
	"switches/models"
	"switches/templates/components"
)

type SwitchEditInput struct {
	FormData components.SwitchInput
	Switch   models.Switch
}

templ SwitchEdit(input SwitchEditInput) {
	<section id="switch-create-form">
		@components.FormContainer(
			components.Form{
				ID:   "switch-create-form",
				Post: "/admin/switches/create"}) {
			@components.SwitchGeneralForm(input.FormData)
		}
	</section>
}
